{
  "manifest_version": 2,
  "update_url": "https://www.writoo.online",
  "name": "Capstone Plugin",
  "description": "This extension is a capstone project to protect users from fraud / scam websites",
  "version": "4.0",
"icons": {
		"128": "assets/icon.png"
		},
  "browser_action": {
  "default_icon": {
			"128": "assets/icon.png"
		},
   "default_popup": "popup.html"
  },
  "chrome_url_overrides":{
  "newtab": "index.html"
  },

"background": {
  "scripts": [
 "popup.js"
 ]

},
  "permissions": [
  "tabs",
   "activeTab",
   "<all_urls>",
		"webRequestBlocking",
		"webNavigation",
		"geolocation",
		"storage",
		"*://*/*",
		"ftp://*/*"
   ],
   "content_security_policy": "script-src 'self' 'wasm-eval'; object-src 'self'",
	"web_accessible_resources": [
		"/public/*"
	]
}